CREATE TABLE rss_status (
    feed_url TEXT PRIMARY KEY,  -- RSS 源 URL，作为主键
    identifier TEXT,          -- 最新条目的唯一标识符
    timestamp TEXT             -- 最新条目的时间戳
);


